<?xml version="1.0" encoding="iso-8859-1"?>

<document>

    <!-- Conversions used to test the ListEventAssembler algorithm. -->
    
    <conversions>

        <!-- DEL event after a DEL event. -->

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="0" y="1"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="0" y="1"/>
                <event type="del" x="1" y="5"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="1" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="1" y="9"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="1" y="2"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="1" y="7"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="3" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="3" y="9"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="4" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="3" y="7"/>
                <event type="del" x="4" y="4"/>
            </output-sequence>
        </conversion>
        
    </conversions>
    
</document>
