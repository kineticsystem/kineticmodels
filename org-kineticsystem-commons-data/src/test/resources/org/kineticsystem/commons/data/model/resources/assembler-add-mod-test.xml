<?xml version="1.0" encoding="iso-8859-1"?>

<document>

    <!-- Conversions used to test the ListEventAssembler algorithm. -->
    
    <conversions>

        <!-- MOD event after a ADD event. -->

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="1" y="2"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="1" y="4"/>
                <event type="mod" x="5" y="6"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="1" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="1" y="4"/>
                <event type="mod" x="5" y="6"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="3" y="5"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="7" y="8"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="7" y="8"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="7" y="8"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="7" y="8"/>
            </output-sequence>
        </conversion>
        
        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="5" y="8"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
                <event type="mod" x="7" y="8"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="4"/>
                <event type="mod" x="2" y="5"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="2" y="3"/>
                <event type="mod" x="4" y="5"/>
            </output-sequence>
        </conversion>

    </conversions>
    
</document>
