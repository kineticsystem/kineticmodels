<?xml version="1.0" encoding="iso-8859-1"?>

<document>

    <!-- Conversions used to test the ListEventAssembler algorithm. -->
    
    <conversions>
        
        <!-- MOD event after a DEL event. -->

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="6"/>
                <event type="mod" x="0" y="1"/>
            </input-sequence>
            <output-sequence>
                <event type="mod" x="0" y="1"/>
                <event type="del" x="3" y="6"/>
            </output-sequence>
        </conversion>
        
        <conversion>
            <input-sequence>
                <event type="del" x="3" y="6"/>
                <event type="mod" x="1" y="2"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="1" y="4"/>
                <event type="mod" x="1" y="2"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="6"/>
                <event type="mod" x="1" y="3"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="1" y="4"/>
                <event type="mod" x="1" y="3"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="3" y="6"/>
                <event type="mod" x="3" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="3" y="6"/>
                <event type="mod" x="3" y="4"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="del" x="2" y="5"/>
                <event type="mod" x="3" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="del" x="2" y="5"/>
                <event type="mod" x="3" y="4"/>
            </output-sequence>
        </conversion>

    </conversions>
    
</document>
