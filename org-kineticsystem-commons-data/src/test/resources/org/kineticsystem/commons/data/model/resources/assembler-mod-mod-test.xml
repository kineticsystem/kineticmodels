<?xml version="1.0" encoding="iso-8859-1"?>

<document>

    <!-- Conversions used to test the ListEventAssembler algorithm. -->
    
    <conversions>

        <!-- MOD event after a MOD event. -->

        <conversion>
            <input-sequence>
                <event type="mod" x="3" y="6"/>
                <event type="mod" x="1" y="1"/>
            </input-sequence>
            <output-sequence>
                <event type="mod" x="1" y="1"/>
                <event type="mod" x="3" y="6"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="mod" x="3" y="6"/>
                <event type="mod" x="1" y="2"/>
            </input-sequence>
            <output-sequence>
                <event type="mod" x="1" y="6"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="mod" x="3" y="6"/>
                <event type="mod" x="7" y="7"/>
            </input-sequence>
            <output-sequence>
                <event type="mod" x="3" y="7"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="mod" x="3" y="6"/>
                <event type="mod" x="8" y="8"/>
            </input-sequence>
            <output-sequence>
                <event type="mod" x="3" y="6"/>
                <event type="mod" x="8" y="8"/>
            </output-sequence>
        </conversion>

    </conversions>
    
</document>
