<?xml version="1.0" encoding="iso-8859-1"?>

<document>

    <!-- Conversions used to test the ListEventAssembler algorithm. -->
    
    <conversions>
    
        <!-- ADD event after an ADD event. -->

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="add" x="0" y="1"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="0" y="1"/>
                <event type="add" x="5" y="8"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="add" x="2" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="2" y="4"/>
                <event type="add" x="6" y="9"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="7"/>
                <event type="add" x="4" y="6"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="10"/>
            </output-sequence>
        </conversion>

        <conversion>
            <input-sequence>
                <event type="add" x="3" y="6"/>
                <event type="add" x="8" y="9"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
                <event type="add" x="8" y="9"/>
            </output-sequence>
        </conversion>
        
        <conversion>
            <input-sequence>
                <event type="add" x="3" y="4"/>
                <event type="add" x="3" y="4"/>
            </input-sequence>
            <output-sequence>
                <event type="add" x="3" y="6"/>
            </output-sequence>
        </conversion>
        
    </conversions>
    
</document>
